  inherited cds: TClientDataSet
    CommandText = 
      'SELECT P.*,U.NOME DESCRI_UNIDADE,'#13#10'x.NOME CONV_DESCRIUNIDADE,'#13#10'F' +
      '.RAZAO_NOME FABRICANTE,G.DESCRI GRUPO,'#13#10'S.DESCRI SUBGRUPO, N.DES' +
      'CRICAO DESCRI_NCM '#13#10'FROM PRODUTO P '#13#10'LEFT OUTER JOIN UNIDADE U O' +
      'N (U.CODIGO = P.COD_UNIDADE) '#13#10'LEFT OUTER JOIN UNIDADE X ON (X.C' +
      'ODIGO = p.CONV_UNIDADE)'#13#10'LEFT OUTER JOIN FABRICANTE F ON (F.CODI' +
      'GO = P.COD_FABRICANTE) '#13#10'LEFT OUTER JOIN GRUPO_PROD G ON (G.CODI' +
      'GO = P.COD_GRUPO) '#13#10'LEFT OUTER JOIN SUBGRUPO_PROD S ON (S.CODIGO' +
      ' = P.COD_SUBGRUPO) '#13#10'LEFT OUTER JOIN NCM N ON (N.NCM = P.COD_NCM' +
      ')  '#13#10'WHERE 1 = 2'
    ProviderName = 'DSPLer1'
    RemoteServer = DM.DSProviderConnection1
    AfterInsert = cdsAfterInsert
